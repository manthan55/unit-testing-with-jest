{
    "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "pretty": true,
      "strict": true,
      "sourceMap": true,
      "target": "ES6",
      "rootDir": "./src",
      "outDir": "./dist",
      "esModuleInterop": true,
      "strictPropertyInitialization": false
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  